[dev]
# 开发环境配置

# 数据库配置
db.host = 127.0.0.1
db.user = root
db.password = xxx
db.port = 3306
db.name = xxxx
db.prefix = pp_
db.timezone = Asia/Shanghai

#DB
db.sl.name = xxx
db.sl.prefix = sl_

# redis config
redis.host = xxxxx
redis.port = xxxx
redis.db = 0
redis.auth = xxxxx

redis.link = xxxxxx:7379

# 钉钉机器人URL
dingTalkRobot = "钉钉机器人URL"

# 密码每多长时间生成一次，支持Linux crontab 的格式
# enableCrontabAuthPassword 为 true的情况下，该配置才有效
# https://github.com/robfig/cron
# 0 30 * * * *
# @hourly
# @every 1h30m
# @daily
# 开发环境每1分钟生成一次验证密码
genAuthPasswordCrontab = @every 1m